    turtle.down()
    turtle.left(25)
    turtle.circle(5, 180)
    turtle.up()
    turtle.setpos(180,45)
    turtle.pd()
    turtle.left(135)
    turtle.circle(5,180)
    turtle.up()
    turtle.setpos(30,-15)
    turtle.pendown()
    turtle.right(90)
    turtle.forward(10)
    turtle.up()
    turtle.setpos(30,-15)
    turtle.pd()
    turtle.right(70)
    turtle.forward(10)
    turtle.left(90)
    turtle.forward(10)
