import turtle
import random
turtle.speed(10)
turtle.ht()
def board():
    turtle.forward(200)
    turtle.left(180)
    turtle.forward(60)
    turtle.right(90)
    turtle.forward(60)
    turtle.left(180)
    turtle.forward(200)
    turtle.left(180)
    turtle.forward(60)
    turtle.right(90)
    turtle.forward(60)
    turtle.left(180)
    turtle.forward(200)
    turtle.left(180)
    turtle.forward(60)
    turtle.right(90)
    turtle.forward(60)
    turtle.left(180)
    turtle.forward(200)
    turtle.penup()
    turtle.setpos(30,55)
    turtle.pd()
    turtle.right(45)
    turtle.forward(5)
    turtle.right(135)
    turtle.forward(10)
    turtle.right(90)
    turtle.forward(5)
    turtle.right(180)
    turtle.forward(10)
    turtle.up()
    turtle.setpos(110,55)
    turtle.pd()
    turtle.right(270)
    turtle.circle(4, 180)
    turtle.up()
    turtle.setpos(109,55)
    turtle.pd()
    turtle.right(45)
    turtle.forward(10)
    turtle.left(135)
    turtle.forward(10)
    turtle.up()
