def circle():
    turtle.circle(15)
def X():
    turtle.left(65)
    turtle.forward(30)
    turtle.left(180)
    turtle.forward(15)
    turtle.right(130)
    turtle.forward(15)
    turtle.left(180)
    turtle.forward(30)
    turtle.left(65)
def box1():
    turtle.up()
    turtle.setpos(30,10)
    turtle.pd()
def box2():
    turtle.up()
    turtle.setpos(100,10)
    turtle.pd()
def box3():
    turtle.up()
    turtle.setpos(180,10)
    turtle.pd()
def box4():
    turtle.up()
    turtle.setpos(30,-65)
    turtle.pd()
def box5():
    turtle.up()
    turtle.setpos(100,-65)
    turtle.pd()
def box6():
    turtle.up()
    turtle.setpos(180,-65)
    turtle.pd()
def box7():
    turtle.up()
    turtle.setpos(30,-135)
    turtle.pd()
def box8():
    turtle.up()
    turtle.setpos(100,-135)
    turtle.pd()
def box9():
    turtle.up()
    turtle.setpos(180,-135)
    turtle.pd()






    
for i in range(1):
    board()
    PlayerChoice = 0
    ComputerChoice = 0
    Symbol = 0
    Gamemode = 0
    Turn = 0
    ComputerMoves = [1,2,3,4,5,6,7,8,9]
    print('Hello and welcome to some Tic-Tac-Toe')
    print()
    print('You can see a grid to the right')
    print('----------------------------------------------------------------->')
    print()
    print('Input the number in order to fill it with your symbol')
    input()
    print()
    print('Alright now choose the gamemode')
    print()
    print('1 for a match against AI                  2 for match against a friend')
    Gamemode = input()
    if Gamemode == "1":
        print('You have chosen AI')
        print()
        print('Choose the symbol used by you')
        print()
        print(' 1 = circle     2 = X')
        Symbol = input()
        if Symbol == "1":
            print('You have chosen circle')
            print('Whoever goes first is randomized')
            input()
            Turn = random.randint(1,2)
            if Turn == 1:
                print('Player 1 goes first.')
                print()
                print('Choice your spot on the grid')
                PlayerChoice = input()
                if PlayerChoice == "1":
                    box1()
                    circle()
                    ComputerMoves.remove(1)
                    random.shuffle(ComputerMoves)
                    ComputerChoice = ComputerMoves.pop()
                    if ComputerChoice == 2:
                        print('Computer chose box 2')
                        box2()
                        X()
                        print('Choice your spot on the grid')
                        PlayerChoice = input()
                        if PlayerChoice == "3":
                            box3()
                            circle()
                            ComputerMoves.remove(3)
                            random.shuffle(ComputerMoves)
                            ComputerChoice = ComputerMoves.pop()
                            if ComputerChoice == 4:
                                print('Computer chose box 4')
                                box4()
                                X()
