        elif Symbol == "2":
            print('You have chosen X')
            print('Whoever goes first is randomized')
            input()
            Turn = random.randint(1,2)
            if Turn == 1:
                print('Player 1 goes first.')
                print()
                print('Choice your spot on the grid')
                PlayerChoice = input()
                if PlayerChoice == "1":
                    box1()
                    circle()
